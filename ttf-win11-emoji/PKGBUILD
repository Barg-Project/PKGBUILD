# validpgpkeys=('43ED9B584913185E7D27A294C382C5DD0557AA1E')

package() {
	install -Dm644 "seguiemj.ttf" "${pkgdir}/usr/share/fonts/${pkgname#ttf-}/Seguiemj.ttf"
}

